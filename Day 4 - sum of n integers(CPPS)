#include <stdio.h>
int input(int n)
{
 printf("Enter the no. of values required\n");
 scanf("%d", &n);
 return n;
}
int store(int i; int a[i])
{
 int m;
 for (m=0; m<i; m++)
 { 
  printf("Enter the integer\n");
  scanf("%d", &a[i]);
 }
int *s = a[i];
return *s;
}
int compute(int i)
{
 int m,sum;
 for (m=0; m<i; m++)
 {
  sum = sum + m;
 }
return sum;
}
int output(int y)
{ 
 printf("The sum of the integers is %d\n", y);
}
int main()
{
 int a;
 int b = input(a);
 int *g = store(b);
 int x = compute(*g);
 output (x);
}
