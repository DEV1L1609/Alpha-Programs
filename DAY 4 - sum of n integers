#include <stdio.h>
#define S 12
int
main (void)
{

  int a[S];

  int n;

  printf ("Enter the value of no. of elements in array\n");

  scanf ("%d", &n);



  for (int i = 0; i < n; i++)

    {

      printf ("Enter the array value\n");

      scanf ("%d", &a[i]);

    }

  int v_sum = 0;

  int i;

  for (i = 0; i < n; i++)

    {

      v_sum = v_sum + a[i];

    }

  printf ("The sum of n integers is %d\n", v_sum);

}
