#include <stdio.h>
#define SIZE 12
int sum(int a[], int n)
{
    int sum = 0;
    int i;
    for (i=0;i<n;i++)
    {
        sum = sum + a[i];
        return sum;
    }
}

int main (void)
{
  int a[SIZE];
  int n;
  for (int i=0; i<n; i++)
  {
     
     printf("Enter the first n integers here\n");
      scanf("%d", &a[i]);
      
  }       
  
}
