#include <stdio.h>
#define S 12
int sum(int S; int i)
{
    int a[ S ];
    size_t i;
    int total=0;
    for (i=0; i<S; i++)
    {
        int total = total + a[i];
    }
}
int main(void)
{
    int j=0;
    int n;
    for (j=0, j<n, j++)
    {
        printf ("%7u%13d\n", j, a[j]);
    }
    
    
}
